<div class="container">
  <table class="table table-bordered">
    <thead>
      <tr>
        <th scope="col">Item Name</th>
        <th scope="col">Ordered by</th>
        <th scope="col">Customer Email</th>
      </tr>
    </thead>
    


<% for (order of orders) {%>
    <tbody>
        <tr>
            <td><%= order.itemName %></td>
            <td><%= order.customerName%></td>
            <td><%= order.customerEmail %></td>
        </tr>
      </tbody>
<% } %>

</table>
</div>