
<form action="/restuarant/create-session" method="POST" class="col-5 forms-container justify-content-center align-items-center">
    <div class="form-group">
        <label for="email">Restuarant email address</label>
        <input type="email" class="form-control" id="email" aria-describedby="emailHelp" name="email" placeholder="Your@email.com" required>
    </div>
    <div class="form-group">
        <label for="password">Password</label>
        <input type="password" class="form-control" id="password" name="password" placeholder="Your password" required>
    </div>
    <input type="hidden" name="userType" value="restuarant">
    <button type="submit" class="btn btn-primary">Submit</button>
</form>
